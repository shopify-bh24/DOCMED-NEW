{%- style -%}
  header-drawer {
    justify-self: start;
  }
  .search  .field_wbinput .field::after,.search  .field_wbinput .field::before{
    border-radius: 25px;
    border: 3px solid #80899B;
  }
  .search  .field_wbinput .field__input:focus-visible, .select__select:focus-visible {
    border-radius: 25px;
    box-shadow: 0 0 0 calc(0.1rem + var(--inputs-border-width)) #80899B;
  }
  .contact_main{
    width: 300px;
    display: flex;
    justify-content: end;
  }
  .contact_main svg{
    width: 30px;
    border: 2px solid black;
    border-radius: 50%;
    padding: 4px;
  }
  .contact_main .contact_text{
   margin-left: 4px;
  }
  .contact_main .contact_text a{
      text-decoration: underline;
    font-weight:500;
    font-size: 14px;
   
  }
  .support_main{
    display: flex;
    flex-direction: row;
    gap: 8px;
    justify-content: center;
  align-items: center;
  }
  .support_main svg{
    width: 30px;
    height: 30px;
    border: 2px solid black;
    border-radius: 50%;
    padding: 4px;
  }
  .support_main .support_text{
    font-weight:500;
    font-size: 14px;
  }
  .hmuser .wbhuserleft{
    display:flex;
  }
  .hmuser .wbhuserleft span:nth-of-type(1){
    font-size: 11px;
    color: #5B5F67;
  }
   
  .hmuser .wbhuserleft span:nth-of-type(2){
     font-weight:500;
     font-size: 14px;
  }
  .wbhcart .wbcarthtotal  span:nth-of-type(1) {
    font-size: 12px;
    color: #5B5F67;
  }
  .wbhcart .wbcarthtotal span:nth-of-type(2){
    display: block ruby;
     font-weight:500;
     font-size: 14px;
  }
  .header  .header__icons{
    column-gap: 30px;
  }
  {%- if section.settings.head_shadow == 'head_shadow_all' -%}
  .header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  {%- endif -%}
  {%- if section.settings.head_shadow == 'head_shadow_home' -%}
  .header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  .page-index .header-wrapper {
    box-shadow: none;
  }
  {%- endif -%}
  .shopify-section-header-sticky sticky-header.header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' and section.settings.head_shadow == 'head_shadow_home' -%}
    .page-index .shopify-section-header-sticky sticky-header.header-wrapper {
      box-shadow: none;
    }
  {%- endif -%}
  {%- if section.settings.sticky_header_type == 'always' and section.settings.head_shadow == 'head_shadow_home' -%}
    .page-index .shopify-section-header-sticky sticky-header.header-wrapper {
      box-shadow: none;
    }
  {%- endif -%}
  .shopify-section-header-sticky.scrolled-past-header sticky-header.header-wrapper {
    box-shadow: 0 6px 12px -12px rgba(var(--color-shadow),0.6);
  }
  {%- if section.settings.logo_position == 'top-left' -%}
    .psearch-hidden {
      overflow: hidden;
    }
    .psearch-hidden:before{
      content: '';
      position: fixed;
      z-index: 3;
      left: 0;
      top: 0;
      width: 100vw;
      height: 100%;
      display: flex;
      justify-content: flex-end;
      background-color: rgba(var(--color-foreground),.3);
    }
    .psearch-hidden .section-header{
      position: unset;
    }
    .psearch-hidden .header--top-left .header__search {
      z-index: 3;
    }
    .psearch-hidden .section-header.shopify-section-header-sticky{
      position: sticky;
      top: 0;
    }
  {%- endif -%}
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
    .scrolled-past-header .header__heading-logo-wrapper {
      width: 75%;
    }
  {%- endif -%}
    .header{
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  @media screen and (min-width: 992px) {
    header-drawer.mobile-drawer {
      display: none;
    }
  }
  {%- if section.settings.menu == blank -%}
    @media screen and (min-width: 992px) {
      .gradient.wbmenuup.header__inline-menu {
        padding: 0;
      }
    }
  {%- endif -%}
  @media screen and (max-width: 991px) {
    .header.header--top-left details-modal {
      margin-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    }
    .header--top-left .predictive-search.predictive-search--header,
    .header--middle-left.search-full-width .predictive-search.predictive-search--header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
    .header{
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }
  }
  .section-header {
    position: sticky;
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }
  predictive-search:not([loading]) .predictive-search__loading-state, predictive-search:not([loading]) .predictive-search-status__loading {
      display: none;
  }
  @media screen and (min-width: 768px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
 @media screen and (max-width: 450px) {
  body .header .header__icons {
    column-gap: 5px;
}
 
 
 }
